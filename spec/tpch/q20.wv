from
    supplier,
    nation
where
    s_suppkey.in(
        from partsupp
        where
            ps_partkey.in(
                from part
                where p_name.like('forest%')
                select p_partkey
            )
            and ps_availqty > {
                 from lineitem
                 where
                    l_partkey = ps_partkey
                    and l_suppkey = ps_suppkey
                    and l_shipdate >= '1994-01-01'::date
                    and l_shipdate < '1995-01-01'::date
                 select 0.5 * l_quantity.sum
            }
        select ps_suppkey
    )
    and s_nationkey = n_nationkey
    and n_name = 'CANADA'
select
    s_name,
    s_address
order by
    s_name
