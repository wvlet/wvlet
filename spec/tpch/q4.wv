from orders
where
    o_orderdate >= '1993-07-01'::date
    and o_orderdate < '1993-10-01'::date
    and exists {
        from lineitem
        where l_orderkey = o_orderkey and l_commitdate < l_receiptdate
    }
group by o_orderpriority
select
    o_orderpriority,
    order_count = _.count
order by
    o_orderpriority
