from
    supplier,
    (
        from lineitem
        where
            l_shipdate >= '1996-01-01'.to_date
            and l_shipdate < '1996-04-01'.to_date
        group by supplier_no
        select
            supplier_no: l_suppkey,
            total_revenue: (l_extendedprice * (1 - l_discount)).sum
    ) as revenue0
where
    s_suppkey = supplier_no
    and total_revenue = (
        from (
            from lineitem
            where
                l_shipdate >= '1996-01-01'.to_date
                and l_shipdate < '1996-04-01'.to_date
            group by supplier_no
            select
                supplier_no: l_suppkey,
                total_revenue: (l_extendedprice * (1 - l_discount)).sum
           ) as revenue1
        select
            total_revenue.max
    )
select
    s_suppkey,
    s_name,
    s_address,
    s_phone,
    total_revenue
order by
    s_suppkey
