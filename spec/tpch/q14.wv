from
    lineitem,
    part
where
    l_partkey = p_partkey
    and l_shipdate >= '1995-09-01'::date
    and l_shipdate < '1995-10-01'::date
select
    promo_revenue =
      100.00 * (if p_type.like('PROMO%') then l_extendedprice * (1 - l_discount) else 0).sum /
        (l_extendedprice * (1 - l_discount)).sum
