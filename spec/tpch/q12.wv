from
    orders,
    lineitem
where
    o_orderkey = l_orderkey
    and l_shipmode in ('MAIL', 'SHIP')
    and l_commitdate < l_receiptdate
    and l_shipdate < l_commitdate
    and l_receiptdate >= '1994-01-01'::date
    and l_receiptdate < '1995-01-01'::date
group by
    l_shipmode
select
    l_shipmode,
    high_line_count =
      (if o_orderpriority = '1-URGENT' or o_orderpriority = '2-HIGH' then 1 else 0).sum,
    low_line_count =
      (if o_orderpriority != '1-URGENT' and o_orderpriority != '2-HIGH' then 1 else 0).sum,
order by
    l_shipmode
